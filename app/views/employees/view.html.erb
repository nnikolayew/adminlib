<p id="notice"><%= notice %></p>

<h1>Listing Employees</h1>

<table>
  <thead>
    <tr>
      <th>Surname</th>
      <th>Name</th>
      <th>Lastname</th>
      <th>Birthday</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @employees.each do |employee| %>
      <tr>
        <td><%= employee.surname %></td>
        <td><%= employee.name %></td>
        <td><%= employee.lastname %></td>
        <td><%= employee.birthday %></td>
      </tr>
    <% end %>
  </tbody>
</table>

